{
    "name": "brandembassy/ui-components-nette-form",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BrandEmbassy\\Components\\NetteForm\\": "src"
        }
    },
    "require": {
        "php": ">=7.4",
        "beberlei/assert": "^3",
        "brandembassy/ui-components": "^6.0",
        "nette/forms": "^3.0.0"
    },
    "require-dev": {
        "brandembassy/coding-standard": "^8.6",
        "brandembassy/mockery-tools": "3.9",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8",
        "roave/security-advisories": "dev-master"
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs --standard=ruleset.xml src",
        "phpcbf": "./vendor/bin/phpcbf --standard=ruleset.xml src",
        "phpstan": "./vendor/bin/phpstan analyse src --memory-limit=-1",
        "test": "./vendor/bin/phpunit src"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "process-timeout": 600,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
