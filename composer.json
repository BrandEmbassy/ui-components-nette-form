{
    "name": "brandembassy/ui-components-nette-form",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BrandEmbassy\\Components\\NetteForm\\": "src"
        }
    },
    "require": {
        "php": "^7.2",
        "beberlei/assert": "^3",
        "brandembassy/ui-components": "^3.7|^4.0|dev-ara-de5407-pagination-component",
        "nette/forms": "v3.0.0"
    },
    "require-dev": {
        "brandembassy/coding-standard": "^5.3",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8",
        "roave/security-advisories": "dev-master"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/BrandEmbassy/ui-components-nette-form"
        }
    ],
    "scripts": {
        "phpcs": "./vendor/bin/phpcs --standard=ruleset.xml src",
        "phpcbf": "./vendor/bin/phpcbf --standard=ruleset.xml src",
        "phpstan": "./vendor/bin/phpstan analyse src",
        "test": "./vendor/bin/phpunit src"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "process-timeout": 600
    }
}
